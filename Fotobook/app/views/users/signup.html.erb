 <% content_for :css do
  stylesheet_link_tag 'users'
  end %>
  <div class="container">
    <div class="row">
      <div class="col-sm-6 col-md-4 col-md-offset-4 m-auto pt-4">
        <h1 class="text-center login-title text-primary">Fotobook Signup</h1>
        <div class="form-icon border">
          <div class="row">
            <div class="col-4 col-xs-8">
              <a href="#"><img src="/assets/google.png" class="rounded-circle img-fluid"></a>
            </div>
            <div class="col-4 col-xs-8">
              <a href="#"><img src="/assets/facebook.png" class="rounded-circle img-fluid"></a>
            </div>
            <div class="col-4 col-xs-8">
              <a href="#"><img src="/assets/twitter.png" class="rounded-circle img-fluid"></a>
            </div>
          </div>
        </div>
            <div class="account-wall">
              <form class="form-signin">
                <label>First Name</label>
                <input type="text" class="form-control" placeholder="First Name" required autofocus>
                <label>Last Name</label>
                <input type="text" class="form-control" placeholder="Last Name" required>
                <label>Email</label>
                <input type="Email" class="form-control" placeholder="Email" required>
                <label>Password</label>
                <input type="password" class="form-control" placeholder="Password" required>
                <div class="clearfix"></div>
                <div class="text-center">
                  <button class="btn btn-lg btn-primary" type="submit">
                  Signup</button>
                </div>
              </form>
            </div>

      </div>
    </body>
    </html>
